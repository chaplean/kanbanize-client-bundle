{
    "name": "chaplean/kanbanize-api-client-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Kanbanize Api client",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "https://www.chaplean.coop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\KanbanizeApiClientBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Chaplean\\Bundle\\KanbanizeApiClientBundle\\": "Tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/config": "^3.0 || ^4.0 || ^5.0",
        "symfony/dependency-injection": "^3.0 || ^4.0 || ^5.0",
        "symfony/yaml": "^3.0 || ^4.0 || ^5.0",
        "chaplean/api-client-bundle": "^1.1"
    },
    "require-dev": {
        "chaplean/coding-standard": "^1.1",
        "mockery/mockery": "^1.2",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^5.0 || ^6.0 || ^7.0 || ^8.0",
        "symfony/http-kernel": "^2.8 || ^3.0 || ^4.0 || ^5.0",
        "symfony/var-dumper": "^2.8 || ^3.0 || ^4.0 || ^5.0"
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "sort-packages": true
    }
}
